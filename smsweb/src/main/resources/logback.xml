<configuration>
	<appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%n-- %date{[yyyy-MM-dd] HH:mm:ss.SSS} assets-%X{traceId} [%thread]-[%file:%line]-[%level] %msg%n</pattern>
		</encoder>
	</appender>
	<!--
	<logger name="com.siloyou.core" level="debug" />
	<logger name="com.siloyou.jmsg" level="debug" />
	-->
	<root level="INFO">
		<appender-ref ref="stdout"/>
	</root>
</configuration>